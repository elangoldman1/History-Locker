{
    "manifest_version": 3,
    "name": "HistoryLocker",
    "version": "1.0",
    "permissions": [
        "history",
        "contentSettings",
        "tabs"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "chrome_url_overrides": {
        "history": "temp.html"
    }
}


